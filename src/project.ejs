---
layout: detail.ejs
pagination:
  data: collections.projects
  size: 1
  alias: project
  permalink: "/projects/<%= project.name %>/index.html"
---

<main>
  <article>
    <h1><%= project.name %></h1>
    <!-- <img src="<%= project.img %>" alt="<%= project.name %>"> -->
    <div class="content">
      <%- project.html %>
    </div>
    <nav class="nav">
      <ul>
          <% project.nav.forEach((heading) => { %>
              <li data-tag="<%= heading.tag %>">
                  <a href="#<%= heading.target %>">
                      <%= heading.text %>
                  </a>
              </li>
          <% }); %>
      </ul>
  </nav>
  <a href="/">â‡§<span class="visually-hidden">Back</span></a>
  </article>
</main>
